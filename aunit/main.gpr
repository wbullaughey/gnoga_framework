with "ada_lib";
with "ada_lib_aunit_lib";
with "application_library";
with "aunit";
with "base";
with "framework";
with "gnoga";
with "root";
with "main_lib";

project Main is
   for Languages use ("Ada");
   for Exec_Dir use Base.Project_Directory;
   for Main use ("main.adb");
   for Object_Dir use Root.Object_Directory;


   package Compiler renames Root.Compiler;
   package Builder is
      for Executable ("main.adb") use "bin/gnoga_aunit";
   end Builder;
end Main;
